{
  "name": "clangcomplete",
  "displayName": "ClangComplete",
  "description": "C/C++ completion using Clang",
  "author": "Christophe Feijoo",
  "license": "MIT",
  "version": "0.1.0",
  "publisher": "kube",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kube/vscode-clang-complete"
  },
  "bugs": {
    "url": "https://github.com/kube/vscode-clang-complete/issues"
  },
  "homepage": "https://github.com/kube/vscode-clang-complete",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp"
  ],
  "main": "./build/client.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Server Configuration",
      "properties": {
        "clangComplete.maxNumberOfProblems": {
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        }
      }
    }
  },
  "scripts": {
    "test": "jest",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "compile": "webpack --config config/webpack.config.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.1",
    "@types/webpack": "^4.4.5",
    "fs-extra": "^6.0.1",
    "jest": "^23.2.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "vsce": "^1.43.0",
    "vscode": "^1.1.18",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "vscode-languageclient": "^4.2.1",
    "vscode-languageserver": "^4.2.1",
    "when-switch": "^0.3.1"
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
